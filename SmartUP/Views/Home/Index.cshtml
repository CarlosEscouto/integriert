@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="pt-bt">
<head>
    <meta charset="UTF-8">
    <title>SmartUP - Soluções inteligentes para condomínios!</title>
    <link rel="icon" href="~/content/images/icon.ico" type="image/x-icon" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="~/content/css/normalize.css" media="screen,projection" />
    <link type="text/css" rel="stylesheet" href="~/content/css/jquery-ui.css" media="screen,projection" />
    <link type="text/css" rel="stylesheet" href="~/content/css/materialize.min.css" media="screen,projection" />
    <link type="text/css" rel="stylesheet" href="~/content/css/dataTables.min.css" media="screen,projection" />
    <link type="text/css" rel="stylesheet" href="~/content/css/style.css" media="screen,projection" />

    <meta charset="utf-8">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>
    <div class="row" style="margin-bottom: 0px; ">
        <nav class="col s12" style="">
            <div class="nav-wrapper">
                <a href="#!" class="brand-logo"><img src="~/content/images/smart.jpg" width="200" style="margin-top: 9px;"></a>
            </div>
        </nav>
    </div>
    <section>
        <div class="row">
            <div class="col s12 m6 offset-m3 l4 offset-l4 xl4 offset-xl4" style="position: absolute; height: 320px; top: calc(50vh - 175px);">
                <div class="card-panel">
                    <div class="center-align" style="margin-bottom: 20px;">
                        <img src="images/smart.jpg" width="180" style="">
                    </div>
                    <form action="@Url.Action("Autenticacao")s" method="GET">
                        <div class="row" style="margin-bottom: 0px;">
                            <div class="input-field col s12">
                                <span class="erro">Erro ao executar login</span>
                                <i class="material-icons prefix">account_circle</i>
                                <input id="user_id" type="text" name="user.login" class="validate">
                                <label for="user_id">User</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <i class="material-icons prefix">vpn_key</i>
                                <input id="user_pass" type="password" name="user.pass" class="validate">
                                <label for="user_pass">Password</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col s12">
                                <input type="submit" name="" value="Logar" style="width: 100%; height: 50px; border: none; background: #1091f7; color: #fff;">
                                
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="fundo"></div>
    </section>
    <!--<script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>-->
    <script type="text/javascript" src="~/scripts/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="~/scripts/jquery-ui.js"></script>
    <script type="text/javascript" src="~/scripts/materialize.min.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.mask.min.js"></script>
    <script type="text/javascript" src="~/scripts/datatables.min.js"></script>
    <script type="text/javascript" src="~/scripts/script.js"></script>
</body>
</html>