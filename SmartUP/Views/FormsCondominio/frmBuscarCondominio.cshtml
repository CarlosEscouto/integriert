<section class="content " style="top:50px; position: relative;">
	<div class="row">
		<div class="col s12 compensa">
			<div class="col s12" style="padding: 0 20px;">
				<style type="text/css">
					.span_forms{ padding: 0 10px; font-size: 16px;}
					.span_forms.ativo{ color: #444; }
				</style>
				<ul class="tab" style="margin-top: 0;">
					<li class="tab col s12 " style="background-color: #f6f6f6; padding: 10px 5px; border-radius: 10px;" >
                        <a class="" href="@Url.Action("Dash", "Home")">
                            <span class="span_forms">Home</span>
                        </a>
						/
						<a class="span_forms ativo" >
							<span class="span_forms">Buscar condomínio</span>
						</a>					
					</li>
				</ul>
			</div>
		</div>
	</div >

	<div class=" compensa">
		<section class="row gutter_form">
			<div class="row">
				<div class="col s12 " style="padding: 10px 0px; border-radius: 10px;">
					<div class="col s12" style="">
						<form class="">
							<div class="row">
								<div class="input-field col s12 m6 l4" style="top: 10px; position: relative;">
									<i class="material-icons prefix" style="font-size: 38px;">search</i>
									<input id="icon_prefix" type="text" class="validate">
                                    <label for="icon_prefix">Procure pelo nome</label>
								</div>
							</div>
						</form>
						<div class="input-field col s5 m4 l3" style="top: -10px; position: relative;">
							<a  href="@Url.Action("frmCadastrarCondominio", "FormsCondominio")" class="btn botao" >Novo</a>
						</div>
					</div>
				</div>
			</div>
            <style>
                .dataTables_length > label{color:#f6f6f6;}
                .dataTables_length > label > div{color: #777;}
            </style>
			<div class="row">
				<div class="col s12 m12 l12 ">
					<div style="padding: 0 22px; overflow: scroll;" class="table_form" >
						<table class="striped datatable">
							<thead>
								<tr>
									<th>Nome</th>
									<th>Endereço</th>
									<th>Quantidade de Blocos</th>
									<th>Proprietario</th>
                                    <th>Inauguração</th>
								</tr>
							</thead>
                            <tfoot>
                                <tr>
                                    <th>Nome</th>
                                    <th>Endereço</th>
                                    <th>Quantidade de Blocos</th>
                                    <th>Proprietario</th>
                                    <th>Inauguração</th>
                                </tr>
                            </tfoot>
							<tbody>
								@foreach (var cond in ViewBag.lstCond)
                                {
                                    <tr>
                                        <td>@cond.nome</td>
                                        <td><a class="modal-trigger" href="#modal1">Endereço</a></td>
                                        <td>@cond.qtdBlocos</td>
                                        <td>@cond.proprietario</td>
                                        <td>@cond.dataInauguracao</td>
                                    </tr>
                                }
							</tbody>
						</table>
					</div>
				</div>
			</div>

		</section>
        <!-- Modal Structure -->
        <div id="modal1" class="modal">
            <div class="modal-content">
                <div class="row">
                    <h5 style="">Endereço</h5>
                    <div class="col s12">
                        <ul class="tabs">
                            <li class="tab col s3"><a href="#test1">Test 1</a></li>
                            <li class="tab col s3"><a class="active" href="#test2">Test 2</a></li>
                        </ul>
                    </div>
                    <div id="test1" class="col s12">
                        <form action="">
                            <div class="row">
                                <div class="input-field col s12 m3">
                                    <input id="end.logradouro" name="end.logradouro" type="text" disabled>
                                    <label for="end.logradouro">Logradouro</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.numero" name="end.numero" type="text" disabled>
                                    <label for="end.numero">Numero</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.complemento" name="end.complemento" type="text" disabled>
                                    <label for="end.complemento">Complemento</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.bairro" name="end.bairro" type="text" disabled>
                                    <label for="end.bairro">Bairro</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s12 m3">
                                    <input id="end.cep" name="end.cep" type="text" disabled>
                                    <label for="end.cep">CEP</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.cidade" name="end.cidade" type="text" disabled>
                                    <label for="end.cidade">Cidade</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.estado" name="area.estado" type="text" disabled>
                                    <label for="end.estado">Estado</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.desc" name="end.desc" type="text" disabled>
                                    <label for="end.desc">Descrição</label>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div id="test2" class="col s12">
                        <form action="">
                            
                            <div class="row">
                                <div class="input-field col s12 m3">
                                    <input id="end.logradouro" name="end.logradouro" type="text" disabled>
                                    <label for="end.logradouro">Logradouro</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.numero" name="end.numero" type="text" disabled>
                                    <label for="end.numero">Numero</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.complemento" name="end.complemento" type="text" disabled>
                                    <label for="end.complemento">Complemento</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.bairro" name="end.bairro" type="text" disabled>
                                    <label for="end.bairro">Bairro</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s12 m3">
                                    <input id="end.cep" name="end.cep" type="text" disabled>
                                    <label for="end.cep">CEP</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.cidade" name="end.cidade" type="text" disabled>
                                    <label for="end.cidade">Cidade</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.estado" name="area.estado" type="text" disabled>
                                    <label for="end.estado">Estado</label>
                                </div>
                                <div class="input-field col s12 m3">
                                    <input id="end.desc" name="end.desc" type="text" disabled>
                                    <label for="end.desc">Descrição</label>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">Fechar</a>
            </div>
        </div>

	</div>
</section>
